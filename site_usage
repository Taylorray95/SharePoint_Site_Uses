Param (
[Parameter(Mandatory=$false,HelpMessage='The site collection search pattern e.g: Texas')][string]$SitesPattern="*",
[Parameter(Mandatory=$false,HelpMessage='The file name to save the CSV file e.g: c:\temp\usage.csv')][string]$FilePath="$PSScriptRoot\usage_$((Get-Date).Ticks.ToString("x16")).csv",
[Parameter(Mandatory=$false,HelpMessage='Days to process. e.g: 180')][int]$DaysToProcess=30
)

Import-Module Microsoft.Graph.Sites

Connect-MgGraph -Scopes "Sites.Read.All"

$sites = Get-MgSite -Search $SitesPattern
Write-Host "Found $($sites.Count) site collections  for pattern '$SitesPattern'";
$i=0;
$days = @()
foreach($site in $sites) {
$i++;
Write-Host "$([string]::new("*", 80))";
Write-Host "[$($i+1)/$($sites.Count) Processing Site $($site.WebUrl)....]";
$startTime = (Get-Date).AddDays(-$DaysToProcess).ToString("u");
$endTime = (Get-Date).ToString("u");
$activities = Get-MgSiteActivityByInterval -SiteId $site.Id -StartDateTime $startTime -EndDateTime $endTime -Interval 'day';
$validDays = $validDays = $activities | ? { $_.access.actionCount; };
Write-Host "   +-- Found $($validDays.Count) days with activitity";
if($validDays.Count -eq 0) {
    Write-Host "   +---- Skipping this site since there is no active day";
}
$items = $validDays | % { [PSCustomObject]@{ 
                        "Site Title"=$site.Name;
                        "Date"=$_.StartDateTime;
                        "Hits"=$_.Access.ActionCount;
                        "Actors"=$_.Access.ActorCount;
                        "SecondsSpent"=$_.Access.AdditionalProperties["timeSpentInSeconds"];
                        "Url"=$site.WebUrl; 
                        }}; 

$days+=$items;
Write-Host "";
}
if(-not $FilePath.ToLower().EndsWith(".csv")) {
   $FilePath = "$FilePath.csv";
} 
Write-Host ""
Write-Host "Saving to file $FilePath";
$days.SyncRoot | Export-Csv -Path $FilePath -Force -Encoding UTF8

